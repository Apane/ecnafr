<div class="landing-bg2 size">
<div class="container-fluid">

<div class="row">
  <div class="push-down">
  <div class="col-xs-4 col-md-4"></div>

<div class="col-xs-4 col-md-4">
  <h2 style="text-align: center; color: white;">
    <%= form_for @quote do |f| %>
    <% if @quote.errors.any? %>
      <div id="error_explanation">
      <h2><%= pluralize(@quote.errors.count, "error") %>
      prohibited this ticket from being created.</h2>
  <ul>
    <%= @ticket.errors.full_messages.each do |msg| %>
    <li><%= msg %></li>
    <% end %>
  </ul>
    </div>
  <% end %>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :body %><br />
    <%= f.text_area :body %>
  </p>
  <p>
    <%= f.submit "Submit Quote" %>
  </p>
  <% end %>
    </h2>
  </div>
  <br/><br/><br/><br/>

  <div class="col-xs-4 col-md-4"></div>
  </div>
</div>

</div>
</div>
