body {
  height: 100%;
  margin: 0;
  padding: 0;
}  

.push-down {
	margin-top: -10%;
}

.navbar-inner {
    background:transparent;
}

@font-face {
  font-family: 'cavier';
  src: font-url('caviardreams-webfont.eot');
  src: font-url('caviardreams-webfont.eot') format('embedded opentype'),
 	   font-url('caviardreams-webfont.woff') format('woff'),
       font-url('caviardreams-webfont.ttf') format('truetype'),
       font-url('caviardreams-webfont.svg#cavier') format('svg');
}

.navbar {
   background-color: transparent;
   background: transparent;
   border-bottom: 0px solid;
}

.navbar li { color: #000 } 
.navbar-inverse .navbar-nav > li > a {
max-width: 200px;
overflow: hidden;
white-space: nowrap;
-webkit-transition: all .25s cubic-bezier(.165,.84,.44,1),opacity 1ms linear;
-moz-transition: all .25s cubic-bezier(.165,.84,.44,1),opacity 1ms linear;
transition: all .25s cubic-bezier(.165,.84,.44,1),opacity 1ms linear;
color: #36465d;
border-color: rgba(0,0,0,.2);
background: rgba(255,255,255,.5);
}

.navbar-inverse .navbar-nav > li > a:hover {
max-width: 200px;
overflow: hidden;
white-space: nowrap;
-webkit-transition: all .25s cubic-bezier(.165,.84,.44,1),opacity 1ms linear;
-moz-transition: all .25s cubic-bezier(.165,.84,.44,1),opacity 1ms linear;
transition: all .25s cubic-bezier(.165,.84,.44,1),opacity 1ms linear;
color: #36465d;
border-color: rgba(0,0,0,.2);
background: rgba(255,255,255,.5);
}

.navbar-inverse .navbar-nav > li > a:active {
max-width: 200px;
overflow: hidden;
white-space: nowrap;
-webkit-transition: all .25s cubic-bezier(.165,.84,.44,1),opacity 1ms linear;
-moz-transition: all .25s cubic-bezier(.165,.84,.44,1),opacity 1ms linear;
transition: all .25s cubic-bezier(.165,.84,.44,1),opacity 1ms linear;
color: #36465d;
border-color: rgba(0,0,0,.2);
background: rgba(255,255,255,.5);
}

.navbar-inverse .navbar-nav > li > a:visited {
max-width: 200px;
overflow: hidden;
white-space: nowrap;
-webkit-transition: all .25s cubic-bezier(.165,.84,.44,1),opacity 1ms linear;
-moz-transition: all .25s cubic-bezier(.165,.84,.44,1),opacity 1ms linear;
transition: all .25s cubic-bezier(.165,.84,.44,1),opacity 1ms linear;
color: black;
border-color: rgba(0,0,0,.2);
background: rgba(255,255,255,.5);
}

.navbar-nav > li > a {
    line-height: 10px;
}    

